{
  "$schema": "http://localhost:3000/schemas/v1.json#",
  "name": "footer-component",
  "version": "2.0.3",
  "namespace": "stanford-components",
  "description": "Stanford university Footer component.",
  "displayName": "Footer",
  "mainFunction": "main",
  "environment": [
    {
      "name": "CONTENT_API",
      "required": true
    },
    {
      "name": "CONTENT_API_KEY",
      "required": true
    },
    {
      "name": "FB_JSON_URL",
      "required": true
    }
  ],
  "icon": {
    "id": "foundation",
    "color": {
      "type": "enum",
      "value": "gray"
    }
  },
  "functions": [
    {
      "name": "main",
      "entry": "server.cjs",
      "input": {
        "type": "object",
        "properties": {
          "dataUrl": {
            "type": "string"
          }
        },
        "required": [
          "dataUrl"
        ]
      },
      "output": {
        "responseType": "html",
        "staticFiles": []
      }
    }
  ],
  "staticFiles": {
    "locationRoot": ""
  },
  "previews": {
    "default": {
      "functionData": {
        "main": {
          "inputData": {
            "type": "file",
            "path": "preview.data.json"
          },
          "wrapper": {
            "path": "outerlayout.html"
          }
        }
      }
    }
  }
}