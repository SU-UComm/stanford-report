{
  "$schema": "http://localhost:3000/schemas/v1.json#",
  "name": "basic-story-hero",
  "version": "2.0.3",
  "namespace": "stanford-components",
  "description": "The basic story hero appears on basic story pages",
  "displayName": "Basic story hero",
  "mainFunction": "main",
  "icon": {
    "id": "flag",
    "color": {
      "type": "enum",
      "value": "pink"
    }
  },
  "functions": [
    {
      "name": "main",
      "entry": "server.cjs",
      "input": {
        "type": "object",
        "properties": {},
        "required": []
      },
      "output": {
        "responseType": "html",
        "staticFiles": []
      }
    }
  ],
  "staticFiles": {
    "locationRoot": ""
  },
  "previews": {
    "default": {
      "functionData": {
        "main": {
          "inputData": {
            "type": "file",
            "path": "preview.data.json"
          },
          "wrapper": {
            "path": "basicstory.html"
          }
        }
      }
    }
  }
}