{
  "$schema": "http://localhost:3000/schemas/v1.json#",
  "name": "acknowledgement",
  "version": "2.0.0",
  "namespace": "stanford-components",
  "description": "A basic WYSIWYG to place an acknowledgement.",
  "displayName": "Acknowledgement",
  "mainFunction": "main",
  "icon": {
    "id": "menu_book",
    "color": {
      "type": "enum",
      "value": "red"
    }
  },
  "functions": [
    {
      "name": "main",
      "entry": "server.cjs",
      "input": {
        "type": "object",
        "properties": {
          "title": {
            "title": "Acknowledgement Title",
            "default": "For more information",
            "description": "This title is read out for screen readers, to provide context and is not displayed visually.",
            "type": "string"
          },
          "content": {
            "title": "Acknowledgement",
            "type": "FormattedText"
          }
        },
        "required": [
          "content",
          "title"
        ]
      },
      "output": {
        "responseType": "html",
        "staticFiles": []
      }
    }
  ],
  "staticFiles": {
    "locationRoot": ""
  },
  "previews": {
    "default": {
      "functionData": {
        "main": {
          "inputData": {
            "type": "file",
            "path": "preview.data.json"
          },
          "wrapper": {
            "path": "basicstory.html"
          }
        }
      }
    }
  }
}