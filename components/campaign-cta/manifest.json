{
  "$schema": "http://localhost:3000/schemas/v1.json#",
  "name": "campaign-cta",
  "version": "0.0.8",
  "namespace": "stanford-components",
  "description": "This component serves as a preview & to promote campaign pages.",
  "displayName": "Campaign CTA",
  "mainFunction": "main",
  "icon": {
    "id": "foundation",
    "color": {
      "type": "enum",
      "value": "gray"
    }
  },
  "functions": [
    {
      "name": "main",
      "entry": "./dist/server.cjs",
      "input": {
        "type": "object",
        "required": [],
        "properties": {
          "displayConfiguration": {
            "title": "Campaign CTA configuration",
            "description": "These options will output to the frontend of the Campaign CTA component",
            "required": ["image"],
            "properties": {
              "image": {
                "title": "Background image",
                "description": "This image will be displayed in the background of the component",
                "type": "string",
                "format": "matrix-asset-uri"
              },
              "title": {
                "title": "Title",
                "description": "The campaign title",
                "type": "string",
                "default": ""
              },
              "description": {
                "title": "Description",
                "description": "The campaign text content",
                "type": "string",
                "format": "multi-line"
              },
              "linkUrl": {
                "title": "CTA Destination",
                "description": "The CTA link destination",
                "type": "string",
                "format": "matrix-asset-uri"
              },
              "linkText": {
                "title": "CTA text",
                "description": "The CTA title",
                "type": "string",
                "default": "Learn more"
              }
            }
          }
        }
      },
      "output": {
        "responseType": "html",
        "staticFiles": [
          {
            "location": "header",
            "file": {
              "type": "css",
              "filepath": "main.css"
            }
          },
          {
            "location": "header",
            "file": {
              "type": "js",
              "filepath": "https://unpkg.com/react@18/umd/react.production.min.js"
            }
          },
          {
            "location": "header",
            "file": {
              "type": "js",
              "filepath": "https://unpkg.com/react-dom@18/umd/react-dom.production.min.js"
            }
          },
          {
            "location": "footer",
            "file": {
              "type": "js",
              "filepath": "client.js"
            }
          }
        ]
      }
    }
  ],
  "staticFiles": {
    "locationRoot": "./dist"
  },
