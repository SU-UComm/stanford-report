{
  "$schema": "http://localhost:3000/schemas/v1.json#",
  "name": "sidebar-navigation",
  "version": "1.0.0",
  "namespace": "stanford-components",
  "description": "A navigation element used for a collection of basic informational pages.",
  "displayName": "Sidebar Navigation",
  "mainFunction": "main",
  "icon": {
    "id": "summarize",
    "color": {
      "type": "hex",
      "value": "#8C1515"
    }
  },
  "functions": [
    {
      "name": "main",
      "entry": "server.cjs",
      "input": {
        "type": "object",
        "properties": {},
        "required": []
      },
      "output": {
        "responseType": "html",
        "staticFiles": []
      }
    }
  ],
  "staticFiles": {
    "locationRoot": ""
  },
  "previews": {
    "default": {
      "functionData": {
        "main": {
          "inputData": {
            "type": "file",
            "path": "preview.data.json"
          },
          "wrapper": {
            "path": "left-sidebar.html"
          }
        }
      }
    }
  }
}